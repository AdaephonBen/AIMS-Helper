{
    "manifest_version": 2,
    "name": "AIMS-Helper",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"],
      "persistent":false
    },
    "page_action": {
      "default_popup":"popup.html "
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": []
        }
      ],
    "permissions": ["declarativeContent", "storage", "tabs", "<all_urls>","http://*/","https://*/"]
}