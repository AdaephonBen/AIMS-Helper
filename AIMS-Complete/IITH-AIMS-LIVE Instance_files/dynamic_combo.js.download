}}}else{openPopupPage({"url":"/help/comboPopupRequest","object":{"object":JSON.stringify(object),"closeMethod":"closeComboPopup"}});}}});}function callComboSetValRequest(object){if(typeof object.beforeCallBack!="undefined"){var object1=eval(object.beforeCallBack+"("+JSON.stringify(object)+");");if(typeof object1=="object"){callAjaxByObj({"url":"/help/comboSetValuesAjax","type":"POST","data":{"object":JSON.stringify(object1)},"resp":afterComboSetvalues(object.id)});
}else{if(object1){callAjaxByObj({"url":"/help/comboSetValuesAjax","type":"POST","data":{"object":JSON.stringify(object)},"resp":afterComboSetvalues(object.id)});}else{return false;}}}else{callAjaxByObj({"url":"/help/comboSetValuesAjax","type":"POST","data":{"object":JSON.stringify(object)},"resp":afterComboSetvalues(object.id)});
}}function afterComboSetvalues(id){return function(response){if(response.uniqueInd){var comboObj={};for(var i in response.comboData){var comboItem=response.comboData[i];comboObj[comboItem.column]=comboItem.value;var focusObj=response.focusObj;if(typeof focusObj!="undefined"){$.each(focusObj,function(id,val){if(id.toLowerCase()==comboItem.column){$("#"+val).val(comboItem.value);
